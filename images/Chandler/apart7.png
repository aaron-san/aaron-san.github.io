<div class="tabContent myPlaces clearfix" id="myPlaces" data-tab-content-id="myPlacesTab">

<div class="content mortar-wrapper">
    <i class="storyicon closeStoryIcon close-btn" id="closeCommuteButton" data-bind="click: doneButtonAction"></i>

    <h2>
        Travel Time
    </h2>
    <p>
        Set a destination, transportation method, and your ideal commute time to see results.
    </p>

    <fieldset class="lookup-group text md">
        <label for="myPlacesLookup" class="my-places-lookup-label">
            Choose a destination
        </label>
        <div class="myPlacesLookup">
            <input type="text" id="myPlacesLookup" autocomplete="off" placeholder="Enter work, school or other address." data-bind="hasfocus: typeaheadHasFocus, css: { error: toShowTAError }">
            <i id="myPlacesSearchIcon" class="searchIcon"></i>
        </div>
        <p class="errorMessage" data-bind="visible: toShowTAError">*Please enter your address or point of interest</p>
    </fieldset>

    <fieldset class="text md">
        <label>Select transportation method</label>
        <div class="travel-type-group">

                <button type="button" class="btn btn-md btn-secondary" data-bind="click: changeTransportation, attr: { value: 1}, css: { active: transportationMethodObservable() === 1 }" name="transportation" id="Transportation_1">
                    <span>Drive</span>
                </button>
                <button type="button" class="btn btn-md btn-secondary" data-bind="click: changeTransportation, attr: { value: 2}, css: { active: transportationMethodObservable() === 2 }" name="transportation" id="Transportation_2">
                    <span>Transit</span>
                </button>
                <button type="button" class="btn btn-md btn-secondary" data-bind="click: changeTransportation, attr: { value: 4}, css: { active: transportationMethodObservable() === 4 }" name="transportation" id="Transportation_4">
                    <span>Walk</span>
                </button>
                <button type="button" class="btn btn-md btn-secondary" data-bind="click: changeTransportation, attr: { value: 3}, css: { active: transportationMethodObservable() === 3 }" name="transportation" id="Transportation_3">
                    <span>Bike</span>
                </button>
        </div>
    </fieldset>

    <div class="floating-containers">
        <fieldset class="text md">
            <label for="idealCommuteTime">Ideal commute time</label>
            <select id="idealCommuteTime" class="selectpicker" type="text" data-bind="value: travelTimeSelected">
                <option value="15" >Under 15 minutes</option>
                <option value="30" >Under 30 minutes</option>
                <option value="45" >Under 45 minutes</option>
                <option value="60" >Under 1 hour</option>
            </select>
        </fieldset>

        <fieldset class="text md" data-bind="visible: (transportationMethodObservable() == 1)">
            <label>
                Set traffic type
            </label>
            <div class="modeOfTransportation rushHour traffic">
                <button type="button" class="btn btn-md btn-secondary" data-bind="css: { active: includeRushHour() }, click: changeRushHour, attr: { value: true }">Rush Hour</button>

                <button type="button" class="btn btn-md btn-secondary" data-bind="css: { active: !includeRushHour() }, click: changeRushHour, attr: { value: false }">No Traffic</button>
            </div>
        </fieldset>
    </div>
    <div class="myCommute added-destinations-container" data-bind="visible: addedPlacesObservable().length > 0">
        <label>Your Destinations</label>

        <ul data-bind="foreach: addedPlacesObservable">
            <li class="clearfix">
                <span class="addressAndTime">
                    <span class="address" data-bind="text: Term, click: $parent.editAddedMyPlace, attr: { title: $root.escapeHtml(Term) }"></span>
                    <span class="transportation" data-bind="text: $root.buildLocalizedSentence(CommuteTime, TransportationMethod, IncludeRushHour)"></span>
                </span>
                <span class="added-places-actions">
                    <i class="storyicon editStoryIcon" data-bind="click: $parent.editAddedMyPlace"></i>
                    <i class="storyicon trashStoryIcon" data-bind="click: $parent.removeAddedMyPlace"></i>
                </span>
            </li>
        </ul>

    </div>



</div>

